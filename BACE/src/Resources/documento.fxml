<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" minHeight="70.0" minWidth="185.0" styleClass="backgroundWhite" stylesheets="@../Style/styles.css" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.Documento">
   <children>
      <Button fx:id="buttonDocumento" minHeight="60.0" minWidth="176.0" mnemonicParsing="false" onAction="#actionDocumento" text="Nombre">
         <font>
            <Font name="Microsoft YaHei Light" size="20.0" />
         </font>
         <graphic>
            <ImageView fx:id="imagenPDF" fitHeight="47.0" fitWidth="47.0" pickOnBounds="true" preserveRatio="true"/>
         </graphic>
         <effect>
            <DropShadow>
               <color>
                  <Color opacity="0.3700000047683716" />
               </color>
            </DropShadow>
         </effect>
      </Button>
   </children>
</VBox>

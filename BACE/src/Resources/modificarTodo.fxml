<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<ScrollPane pannable="true" prefHeight="700.0" prefWidth="1300.0" styleClass="backgroundWhite" stylesheets="@../Style/styles.css" vbarPolicy="ALWAYS" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.ModificarTodo">
   <content>
      <VBox fx:id="vBox" alignment="CENTER" minHeight="700.0" prefWidth="1300.0" spacing="20.0" styleClass="backgroundWhite">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Button fx:id="buttonFinalizar" mnemonicParsing="false" onAction="#actionFinalizar" text="Finalizar">
               <font>
                  <Font name="Microsoft YaHei Light" size="25.0" />
               </font>
               <effect>
                  <DropShadow>
                     <color>
                        <Color opacity="0.30000001192092896" />
                     </color>
                  </DropShadow>
               </effect>
            </Button>
         </children>
      </VBox>
   </content>
</ScrollPane>

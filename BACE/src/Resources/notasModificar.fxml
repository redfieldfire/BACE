<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxWidth="700.0" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.NotasModificar">
   <children>
      <VBox maxWidth="700.0" spacing="20.0" styleClass="backgroundWhite" stylesheets="@../Style/styles.css">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <HBox spacing="20.0">
               <children>
                  <TextField maxWidth="1.7976931348623157E308" promptText="Titulo">
                     <font>
                        <Font name="Microsoft YaHei Light" size="25.0" />
                     </font>
                  </TextField>
                  <Button mnemonicParsing="false" text="Modificar">
                     <font>
                        <Font name="Microsoft YaHei Light" size="23.0" />
                     </font>
                     <effect>
                        <DropShadow>
                           <color>
                              <Color opacity="0.30000001192092896" />
                           </color>
                        </DropShadow>
                     </effect>
                  </Button>
               </children>
            </HBox>
            <TextArea prefHeight="200.0" prefWidth="200.0" promptText="Nota">
               <font>
                  <Font name="Microsoft YaHei Light" size="23.0" />
               </font>
            </TextArea>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
   </children>
   <effect>
      <DropShadow>
         <color>
            <Color opacity="0.3700000047683716" />
         </color>
      </DropShadow>
   </effect>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER" prefHeight="700.0" prefWidth="1200.0" spacing="20.0" styleClass="backgroundWhite" stylesheets="@../Style/styles.css" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.Agregar2">
   <children>
      <VBox alignment="CENTER" prefHeight="460.0" spacing="20.0">
         <children>
            <Button fx:id="buttonAtras" mnemonicParsing="false" onAction="#atrasAction" onMouseEntered="#atrasEntered" onMouseExited="#atrasExited" text="Volver" textAlignment="CENTER">
               <effect>
                  <DropShadow>
                     <color>
                        <Color opacity="0.3700000047683716" />
                     </color>
                  </DropShadow>
               </effect>
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
               <graphic>
                  <ImageView fx:id="imagenVolver" fitHeight="29.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
               <VBox.margin>
                  <Insets right="350.0" />
               </VBox.margin>
            </Button>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <ImageView fx:id="imagenPDF" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="buttonBuscarDoc" mnemonicParsing="false" onAction="#actionBuscarDoc" text="Buscar" textAlignment="CENTER">
                     <effect>
                        <DropShadow>
                           <color>
                              <Color opacity="0.3700000047683716" />
                           </color>
                        </DropShadow>
                     </effect>
                     <font>
                        <Font name="Microsoft YaHei Light" size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="buttonAgregarDoc" mnemonicParsing="false" onAction="#actionAgregarDoc" text="Agregar" textAlignment="CENTER">
                     <effect>
                        <DropShadow>
                           <color>
                              <Color opacity="0.3700000047683716" />
                           </color>
                        </DropShadow>
                     </effect>
                     <font>
                        <Font name="Microsoft YaHei Light" size="20.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" layoutX="30.0" layoutY="333.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre">
                     <font>
                        <Font name="Microsoft YaHei Light" size="25.0" />
                     </font>
                  </Text>
                  <TextField fx:id="textFieldNombreDoc">
                     <font>
                        <Font name="Microsoft YaHei Light" size="23.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="30.0" layoutY="367.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Categoría">
                     <font>
                        <Font name="Microsoft YaHei Light" size="25.0" />
                     </font>
                  </Text>
                  <ComboBox fx:id="comboBoxCategoriaDoc" editable="true" prefWidth="150.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="30.0" layoutY="369.0" spacing="10.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Categoría">
                     <font>
                        <Font name="Microsoft YaHei Light" size="25.0" />
                     </font>
                  </Text>
                  <TextField fx:id="textFieldCategoriaDoc">
                     <font>
                        <Font name="Microsoft YaHei Light" size="23.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </VBox>
      <VBox alignment="CENTER" prefHeight="460.0" prefWidth="370.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre Nota">
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </Text>
            <TextField fx:id="textFieldNombreNota" promptText="Ejemplo: NotaMedica">
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </TextField>
            <Text layoutX="98.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nota">
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </Text>
            <TextArea fx:id="textFieldNota" prefHeight="200.0" prefWidth="200.0" promptText="Notas">
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </TextArea>
            <Button fx:id="buttonAgregarNota" layoutX="145.0" layoutY="234.0" mnemonicParsing="false" onAction="#actionAgregarNota" text="Agregar" textAlignment="CENTER">
               <effect>
                  <DropShadow>
                     <color>
                        <Color opacity="0.3700000047683716" />
                     </color>
                  </DropShadow>
               </effect>
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </Button>
            <Button fx:id="buttonFinalizar" layoutX="111.0" layoutY="389.0" mnemonicParsing="false" onAction="#finalizarAction" onMouseEntered="#finalizarEntered" onMouseExited="#finalizarExited" text="Finalizar" textAlignment="CENTER">
               <effect>
                  <DropShadow>
                     <color>
                        <Color opacity="0.3700000047683716" />
                     </color>
                  </DropShadow>
               </effect>
               <font>
                  <Font name="Microsoft YaHei Light" size="20.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxWidth="700.0" xmlns="http://javafx.com/javafx/1.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Classes.Notas">
   <children>
      <VBox maxWidth="700.0" spacing="20.0" styleClass="backgroundWhite" stylesheets="@../Style/styles.css">
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <children>
            <Text fx:id="textNombreNota" strokeType="OUTSIDE" strokeWidth="0.0" text="Nombre Nota" wrappingWidth="500.0">
               <font>
                  <Font name="Microsoft YaHei Light" size="25.0" />
               </font>
            </Text>
            <Text fx:id="textNota" layoutX="30.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nota" textAlignment="CENTER" wrappingWidth="720.0">
               <font>
                  <Font name="Microsoft YaHei Light" size="25.0" />
               </font>
            </Text>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </VBox>
   </children>
   <effect>
      <DropShadow>
         <color>
            <Color opacity="0.3700000047683716" />
         </color>
      </DropShadow>
   </effect>
</VBox>
